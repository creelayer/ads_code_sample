pipeline{

  agent any
  
  stages{
  
    stage("build"){
    
      steps{
          echo 'run build stage'
      }
    
    }
    
    stage("test"){
    
      steps{
          echo 'run tests'
      }
    
    }
    
    stage("deploy"){
    
      steps{
          echo 'run deploy'
      }
    
    }
  
  }

}
